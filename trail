from tkinter import*
 
def quit ():
    main_window.destroy()
 
def entry_print():
    print(entry_first_name.get())
    print(entry_last_name.get())
    print(entry_recipt_number.get())
    print(entry_item_that_is_hired.get())
    print(entry_item_the_customer_hired.get())
  
 
def main():
    Button(main_window, text="Quit",command=quit) .grid(row=1,column=
    Button(main_window, text="Print",command=entry_print) .grid(row=1,column=6)
    Label(main_window, text="Customer First Name").grid(row=1,column=0)
    Label(main_window, text="Customer last  Name").grid(row=2,column=0)
    Label(main_window, text="recipt number").grid(row=3,column=0)
    Label(main_window, text="item that is hired").grid(row=4,column=0)
    Label(main_window, text="item customer has hired").grid(row=5,column=0)
    main_window.mainloop()
 

def append_name ():
global camp_details,entry_name,entry_age,entry_gender
if len(entry_name.get())!=0 :
    camp_details.append([entry_Customer First Name.get(),entry_Customer last  Name.get(),entry_recipt number.get(),entry_item that is hired.get(),entry_item customer has hired.get()   (])
    Entry_name.delete(0,'end')
    Entry_age.delete(0,'end')
    Entry_gender.delete(0,'end')
    counters['total_entries'] += 1

 
 

main_window=Tk()
Entry_first_name=Entry(main_window)
Entry_first_name.grid(row=1,column=1)
 
 
Entry_last_name=Entry(main_window)
Entry_last_name.grid(row=2,column=1)
 
Entry_recipt_number=Entry(main_window)
Entry_recipt_number.grid(row=3,column=1)
 
 
entry_item_hired=Entry(main_window)
entry_item_hired.grid(row=4,column=1)
 
 
entry_customer_hired=Entry(main_window)
entry_customer_hired.grid(row=5,column=1)
 
 
main()
